{
  "name": "@xtrctio/commitlint-config",
  "version": "1.0.1",
  "author": "Eric Hacke",
  "bugs": {
    "url": "https://github.com/xtrctio/commitlint-config/issues"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^8.0.0",
    "commitizen": "^3.1.1",
    "commitlint": "^8.0.0",
    "cz-conventional-changelog": "^2.1.0",
    "fixpack": "^2.3.1",
    "husky": "^2.2.0"
  },
  "files": [
    "index.js"
  ],
  "homepage": "https://github.com/xtrctio/commitlint-config#readme",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "keywords": [
    "commitlint"
  ],
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xtrctio/commitlint-config.git"
  },
  "scripts": {
    "commit": "git-cz",
    "prepare": "fixpack",
    "test": "echo \"Error: no test specified\" && exit 1"
  }
}
